  "contact" : [
    {
      "name" : "OAAT AG",      
      "telecom" : [
        {
          "system" : "email",
          "value" : "info@ooat-otma.ch"
        }
      ]
    }
  ],
  "jurisdiction" : [
    {
      "coding" : [
        {
          "system" : "urn:iso:std:iso:3166",
