  "content" : "complete",
  "hierarchyMeaning" : "is-a",
  "property": [
    {
      "code": "typ",
      "description": "Typ der Position (Pauschale, Einzel, Zusatz, nicht im Anwendungsbereich)",
      "type": "code"
    },
    {
      "code" : "comment",
      "uri" : "http://hl7.org/fhir/concept-properties#comment",
      "description" : "Medizinische Interpretation",
      "type" : "string"
    },
    {
      "code" : "notSelectable",
      "uri" : "http://hl7.org/fhir/concept-properties#notSelectable",
      "description" : "Selektierbar",
      "type" : "boolean"
    }
  ],
  "concept" : [
    {% assign h1 = 'l1' %}
    {% assign h2 = 'l2' %}
    {% for row in lkaat.rowIndexes() %}  
      {% assign h1row = lkaat.cell(row, 1).substring(0,lkaat.cell(row, 1).indexOf('.')) %}
      {% assign h2row = lkaat.cell(row, 1).substring(lkaat.cell(row, 1).indexOf('.')+1,2) %}
      {% if h1 != h1row %}        
        {% if h2 != 'l2' %}
           ]
          }
        {% assign h2 = 'l2' %}        
        {% endif %}
        {% if h1 != 'l1' %}
          ]
         }
        {% endif %}        
        {
        "code" : "{{ h1row }}",
        "property": [
          {
            "code": "notSelectable",
            "valueBoolean": true
          }
        ],
        "concept" : [        
        {% assign h1 = h1row %}
      {% endif %}      

      {% if h2 != h2row %}        
        {% if h2 != 'l2' %}
          ]
         }
        {% endif %}        
        {
        "code" : "{{ h1row }}.{{ h2row }}",
        "property": [
          {
            "code": "notSelectable",
            "valueBoolean": true
          }
        ],
        "concept" : [        
        {% assign h2 = h2row %}
      {% endif %}      

      {
      "code" : "{{ lkaat.cell(row, 1) }}",
      "display" : "{{ lkaat.cell(row, 3).replace('"','').replace('1 Min.\u00a0', '1 Min. ').trim() }}",
      "property": [
         {
          "code": "typ",
          "valueCode": "{{ lkaat.cell(row, 2) }}"
         }
        {% if lkaat.cell(row, 4)!='-' %}        
          ,
          {
          "code": "comment",
          "valueString": "{{ lkaat.cell(row, 4).replace('{:target="_blank"}','').replace('\\+','\\\\+').trim() }}"
          }
        {% endif %}      
      ]

      },    
    {% endfor %}
    {% if h2 != 'l2' %}
      ]
    }
    {% endif %}
    {% if h1 != 'l1' %}
      ]
    }
    {% endif %}
  ]
}